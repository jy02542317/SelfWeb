@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <style>
        html {
            padding: 0;
            margin: 0;
        }

        body {
            background: #555555;
            color: white;
            padding: 0;
            margin: 0;
        }

        .mynavbar {
            width: 100%;
            position: relative;
            height: 50px;
            position: fixed;
        }

        .logo {
            font-size: 28px;
            color: white;
            font-family: sans-serif;
            font-weight: 500;
            position: relative;
            float: left;
            line-height: 50px;
            margin-left: 20px;
        }

        .date {
            position: relative;
            float: right;
            line-height: 50px;
            margin-right: 20px;
            font-size: 12px;
        }
    </style>
    <title></title>
</head>
<body>
    <div class="mynavbar">
        <div class="logo">
            后台管理系统
        </div>
        <div class="date">
            2017/2/24
        </div>
    </div>
</body>
</html>
